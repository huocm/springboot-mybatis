			                              <s:if test="'${sub.male}'!='1'">
			                              <input type="checkbox">
			                              </s:if>
			                              <s:else>
			                               <input type="checkbox" checked>
			                              </s:else>
			                              ${sub.maleprice }                              
	                              </td>
	                              <td  height="24" align="center" valign="middle" bgcolor="FFFFFF">                       
